#########################
# MentalCore Renderpass #
#########################

declare shader
	material "core_renderpass" (
		array shader	"renderlayer",		#: shortname "rl"
			
		# Render Pass Attributes
		boolean		"enable",				#: shortname "en" default 1
		integer		"type",					#: shortname "ty" default 0 min 0 max 41 enum "Beauty:Colour:Diffuse:Diffuse Raw:Shadow:Shadow Raw:Diffuse Without Shadows:Diffuse Without Shadows Raw:Ambient:Ambient Raw:Indirect:Indirect Raw:Ambient Occlusion:Translucency:Subsurface:Subsurface Front:Subsurface Mid:Subsurface Back:Incandescence:Specular:Reflection:Refraction:Bloom Source:Depth:Normal World:Normal Camera:Point World:Point Camera:Motion Vector:Opacity:Facing Ratio:UV:Material ID:Object ID:Matte:Custom Colour:Custom Vector:Environment:Light Select:Diagnostic Samples:Diagnostic Error:Diagnostic Time"
		integer		"channels",				#: shortname "ch" default 1 min 0 max 2 enum "1:3:4"
		integer		"bit_depth",			#: shortname "bd" default 2 min 0 max 3 enum "8 Bit Integer=0:16 Bit Integer=1:16 Bit Float=2:32 Bit Float=3" 
		integer		"cprofile",				#: shortname "cp" default 0 min 0 max 3 enum "Default Output Profile:Linear:sRGB:Rec. 709"
		boolean		"filtering",			#: shortname "fl" default 1
		string		"group",				#: shortname "gp"
								
		# Light Select Passes
		boolean		"diffuse",				#: shortname "df" default 1
		boolean		"diffuse_raw",			#: shortname "dfr" default 0
		boolean		"shadow",				#: shortname "sh" default 0
		boolean		"shadow_raw",			#: shortname "shr" default 0
		boolean		"diffuse_no_shad",		#: shortname "dfns" default 0
		boolean		"diffuse_no_shad_raw",	#: shortname "dfnsr" default 0
		boolean		"specular",				#: shortname "sp" default 1
			
		# Visiblity Attributes
		boolean		"vis_in_primary",		#: shortname "veye" default 1
		boolean		"vis_in_trans",			#: shortname "vtra" default 1
		boolean		"vis_in_refl",			#: shortname "vrfl" default 0
		boolean		"vis_in_refr",			#: shortname "vrfr" default 0	
						
		# Tonemap Attributes
		scalar		"scale",				#: shortname "sca" default 1.0	
		scalar		"offset",				#: shortname "off" default .0
		boolean		"en_clamp",				#: shortname "enc" default 0
		scalar		"clamp",				#: shortname "clp" default 1.0
						
		# Custom Shader
		shader		"custom_shader",		#: shortname "csh"
							
		# Object/Material/Light Linking
		boolean		"en_obj_linking",		#: shortname "eol" default 0
		integer		"obj_link_mode",		#: shortname "olm" min 0 max 1 enum "Inclusive:Exclusive"
		boolean		"en_light_linking",		#: shortname "ell" default 0
		integer		"light_link_mode",		#: shortname "llm" min 0 max 1 enum "Inclusive:Exclusive"
								
		array geometry	"linked_objs",		#: shortname "lkobj"
		array material	"linked_mats",		#: shortname "lkmat"
		array light		"linked_lights",	#: shortname "lklgt"
								
		#Metadata - only for mr 3.10
		array struct "metadata" {
			boolean		"meta_enable", 			#: shortname "mte" default 1
			integer		"meta_type", 			#: shortname "mtt" default 0 min 0 max 4 enum "String:Boolean:Integer:Scalar:Vector"
			string		"meta_key",				#: shortname "mtk" 
			string		"meta_string",			#: shortname "mtst" 
			boolean		"meta_bool",			#: shortname "mtb" default 0	
			integer		"meta_int",				#: shortname "mti" default 0
			scalar		"meta_scalar",			#: shortname "mtsc" default 0
			vector		"meta_vector"			#: shortname "mtv"
		}, #: shortname "meta"
	)
	version 1
	#: nodeid 818999
end declare

