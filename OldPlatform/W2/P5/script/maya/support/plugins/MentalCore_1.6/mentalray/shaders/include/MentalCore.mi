		scalar		"indirect_scale",			#: shortname "ins" default 1. min 0 softmax 1
		scalar		"indirect_occlusion",		#: shortname "inoc" default 0.5 min 0 max 1
							 
		# Globals Shaders
		shader		"env_shader",				#: shortname "envsh"
		shader		"photographic_shader",		#: shortname "phsh"
		shader		"dof_shader",				#: shortname "dofsh"
				
