		scalar		"envl_scale",			#: shortname "elsc" default .5 min .0 softmax 1.
		scalar		"envl_blur",			#: shortname "elb" default 2.0 min 0.0 softmax 4.0	
		integer		"envl_blur_res",		#: shortname "elbr" default 512 min 0 softmax 1000
		boolean		"envl_en_flood_colour",	#: shortname "elefc" default 0
		color		"envl_flood_colour",	#: shortname "elfc" default .0 .0 .0	

