		boolean		"contrast_all_buffers",		#: shortname "cab" default 0
		boolean		"ray_differentials",		#: shortname "rdif" default 0
		# Only for mr 3.9
		boolean		"hair_mem_mode",			#: shortname "hmem" default 0
		boolean		"hair_caps",				#: shortname "hcap" default 1
		boolean		"finalgather_legacy",		#: shortname "fgl" default 0

		# Unified sampling - only for mr 3.9
		boolean		"unified_sampling",			#: shortname "us" default 0			
		scalar		"samples_min",				#: shortname "umin" default 1. min .1 softmax 100	
		scalar		"samples_max",				#: shortname "umax" default 64. min 1. softmax 100			
		scalar		"samples_quality",			#: shortname "uq" default 0.5 min .0 softmax 2
		scalar		"samples_error_cutoff",		#: shortname "uec" default 0 min .0 softmax 1.
		boolean		"samples_per_object",		#: shortname "upo" default 0

		# Progressive rendering
		boolean		"progressive",				#: shortname "pg" default 0			
		integer		"prog_subsamp_size",		#: shortname "pgss" default 1 min 1
		integer		"prog_subsamp_mode",		#: shortname "pgsm" default 1 min 0 max 1 enum "Sparse:Detail"			
		integer		"prog_subsamp_pattern",		#: shortname "pgsp" default 1 min 0 max 1 enum "Linear:Scatter"	
		integer		"prog_min_samples",			#: shortname "pgmns" default 4 min 0 softmax 100
		integer		"prog_max_samples",			#: shortname "pgmxs" default 100 min 0 softmax 100
		integer		"prog_max_time",			#: shortname "pgmxt" default 0 min 0
		scalar		"prog_error_threshold",		#: shortname "pget" default 0.05 min .0

		boolean		"prog_ao_cache",			#: shortname "pgoc" default 0	
		integer		"prog_ao_cache_points",		#: shortname "pgcp" default 1 min 1
		integer		"prog_ao_cache_rays",		#: shortname "pgcr" default 128 min 0
		integer		"prog_ao_cache_max_frame",	#: shortname "pgcf" default 32
		integer		"prog_ao_cache_exclude",	#: shortname "pgce" default 0

		# Image based lighting
		integer		"ibl_mode",					#: shortname "iblm" default 0 min 0 max 3 enum "Off:Automatic:Approximate:Light"	
		boolean		"ibl_env_cache",			#: shortname "iblec" default 0
		integer		"ibl_env_res",				#: shortname "ibler" default 512 min 1
		integer		"ibl_env_shader_samples",	#: shortname "ibless" default 2 min 1
		scalar		"ibl_quality",				#: shortname "iblq" default .2 min .0 softmax 1.		
		integer		"ibl_shadows",				#: shortname "iblsh" default 2 min 0 max 2 enum "Off:Solid:Transparent"	
		color		"ibl_scale",				#: shortname "iblsc" default 1. 1. 1.
		integer		"ibl_approx_split",			#: shortname "iblsp" default 4 min 0
		integer		"ibl_approx_split_vis",		#: shortname "iblspv" default 2 min 0

		# Stereo rendering
		boolean		"en_stereo_rendering",		#: shortname "est" default 0
		integer		"stereo_method",			#: shortname "stm" default 1 min 0 max 2 enum "Toe In=0:Off Axis=1:Offset=2"
		scalar		"eye_separation",			#: shortname "sts" default .1 min .0
		# Only for mr 3.9
		scalar		"parallax_distance",		#: shortname "stp" default 10.

		# Metadata - only for mr 3.10
		array struct "metadata" {
			boolean		"meta_enable", 			#: shortname "mte" default 1
			integer		"meta_type", 			#: shortname "mtt" default 0 min 0 max 4 enum "String:Boolean:Integer:Scalar:Vector"
			string		"meta_key",				#: shortname "mtk" 
			string		"meta_string",			#: shortname "mtst" 
			boolean		"meta_bool",			#: shortname "mtb" default 0	
			integer		"meta_int",				#: shortname "mti" default 0
			scalar		"meta_scalar",			#: shortname "mtsc" default 0
			vector		"meta_vector"			#: shortname "mtv"
		}, #: shortname "meta"
        
        string		"maya_file_path",           #: shortname "mfp"
        string		"maya_file_name",			#: shortname "mfn" 
							 
		# Other 3.10 options
		integer		"exr_format",						#: shortname "exrf" default 0 min 0 max 1 enum "Scanline:Tiled"
		boolean		"trace_camera_motion_vectors", 		#: shortname "tcmv" default 0
		boolean		"trace_camera_clip",				#: shortname "tccp" default 0
		integer		"finalgather_importance",			#: shortname "fgimp" default 0 min 0 max 2 enum "Off:Partial:Full"
		color		"detail_shadowmap_contrast",		#: shortname "dsmc" default 0. 0. 0.
		scalar		"light_relative_scale",				#: shortname "lrs" default 0.318 min .0
							 
		# AO Trace Flags
		boolean		"ao_flag_visible",			#: shortname "aofv" default 0
		boolean		"ao_flag_shadow",			#: shortname "aofsh" default 1
		boolean		"ao_flag_reflect",			#: shortname "aofrfl" default 0
		boolean		"ao_flag_refract",			#: shortname "aofrfr" default 0
		boolean		"ao_flag_transp",			#: shortname "aoftr" default 0
		boolean		"ao_flag_caustic",			#: shortname "aofcu" default 0
		boolean		"ao_flag_globillum",		#: shortname "aofgi" default 0
		boolean		"ao_flag_finalgather",		#: shortname "aoffg" default 0
		
		# Global Clamp
		boolean		"en_global_clamp",			#: shortname "egc" default 0
		scalar		"global_clamp",				#: shortname "gc" default 1 min 0 max 99999

		# Proxy Textures
		boolean		"use_proxy_textures",		#: shortname "upt" default 1
		boolean		"use_proxy_textures_sec",	#: shortname "upts" default 0
        
        # MR 3.11 Options
        integer		"light_importance_sampling",                 #: shortname "lis" default 0 min 0 max 2 enum "Off:On:Always"
        scalar      "light_importance_sampling_variance",        #: shortname "lisv" default 0.001
        boolean     "light_importance_sampling_precomp",         #: shortname "lisp" default 0
        
        boolean     "multiple_importance_sampling",              #: shortname "mis" default 0
        
        integer		"shutter_shape",			#: shortname "shsp" default 1 min 0 max 1 enum "None (Box):Trapezoidal"
        scalar		"shutter_efficiency",		#: shortname "shef" default 0.8 min .5 max 1.
        
        boolean     "ao_gpu_override_quality",  #: shortname "gpuoq" default 0
        integer		"ao_gpu_passes",            #: shortname "gpup" default 1 min 1
        integer		"ao_gpu_rays",              #: shortname "gpur" default 1 min 1
        
        boolean     "nested_assemblies",        #: shortname "nsa" default 0

        boolean     "preview_profile_pri_buffer",   #: shortname "pppb" default 0
	)
	version 1
	apply geometry
	#: nodeid 819000
end declare

