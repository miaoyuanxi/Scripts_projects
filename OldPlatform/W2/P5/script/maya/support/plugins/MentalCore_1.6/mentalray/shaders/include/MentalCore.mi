		scalar		"bloom_scale",				#: shortname "blsc" default 1. min .0 softmax 4.
		scalar		"bloom_threshold",			#: shortname "blth" default 1. min .0 softmax 4.
		scalar		"bloom_saturation",			#: shortname "blst" default 1. min .0 softmax 4.

		# Pass Switches
		boolean		"en_beauty_passes",		#: shortname "ebp" default 1
		boolean		"en_post_passes",		#: shortname "epp" default 1
		boolean		"en_matte_passes",		#: shortname "emp" default 1
		boolean		"en_custom_passes",		#: shortname "eap" default 1

		# Occlusion
		boolean		"en_ao",				#: shortname "eoc" default 0
		integer		"ao_mode",				#: shortname "aom" default 0 min 0 max 2 enum "MentalCore:Mentalray Built-in:Mentalray GPU"
		color		"ao_colour",			#: shortname "ocl" default .0 .0 .0
		integer		"ao_samples",			#: shortname "ocs" default 32 min 1
		scalar		"ao_spread",			#: shortname "ocsp" default 88. min .0 softmax 100
		scalar		"ao_falloff",			#: shortname "ocfo" default 1 min .0
		scalar		"ao_near_clip",			#: shortname "onc" default 1. min .0 softmax 100
		scalar		"ao_far_clip",			#: shortname "ofc" default 20. min .0 softmax 100
		scalar		"ao_amt_amb_occ",		#: shortname "oaa" default 1.. min .0 max 1
		scalar		"ao_amt_direct_occ",	#: shortname "oad" default .0 min .0 max 1
		
		boolean		"ao_opacity",			#: shortname "oom" default 0
		boolean		"ao_vis_indirect",		#: shortname "ovi" default 0
		boolean		"ao_vis_refl",			#: shortname "ovrf" default 0
		boolean		"ao_vis_refr",			#: shortname "ovrr" default 1
		boolean		"ao_vis_trans",			#: shortname "ovtp" default 1
							 
		integer		"ao_inclexcl_id",		#: shortname "oie" default 0
		integer		"ao_nonself_id",		#: shortname "ons" default 0
							 
